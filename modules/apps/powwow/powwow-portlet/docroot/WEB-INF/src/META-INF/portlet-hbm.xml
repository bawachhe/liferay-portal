<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.powwow.model.PowwowMeeting" />
	<import class="com.liferay.powwow.model.PowwowParticipant" />
	<import class="com.liferay.powwow.model.PowwowServer" />
	<class name="com.liferay.powwow.model.impl.PowwowMeetingImpl" table="PowwowMeeting">
		<cache usage="read-write" />
		<id name="powwowMeetingId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="powwowServerId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="providerType" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="providerTypeMetadata" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="languageId" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="calendarBookingId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.liferay.powwow.model.impl.PowwowParticipantImpl" table="PowwowParticipant">
		<cache usage="read-write" />
		<id name="powwowParticipantId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="powwowMeetingId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="participantUserId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="emailAddress" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="type" type="com.liferay.portal.dao.orm.hibernate.IntegerType" column="type_" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.liferay.powwow.model.impl.PowwowServerImpl" table="PowwowServer">
		<cache usage="read-write" />
		<id name="powwowServerId" type="long">
			<generator class="assigned" />
		</id>
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="providerType" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="url" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="apiKey" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="secret" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="active" type="com.liferay.portal.dao.orm.hibernate.BooleanType" column="active_" />
	</class>
</hibernate-mapping>